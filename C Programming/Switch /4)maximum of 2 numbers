#include<stdio.h>
int main()
{
    int a,b;
     
    printf("Enter a & b number: ");
    scanf("%d%d",&a,&b);
     ;
     
    switch(a>b)
    {
        case 1:
            printf("%d is maximum number\n",a);
            break;
            case 0:
            printf("%d is maximum number\n",b);
            break;
            
    }
     
    return 0;
}
