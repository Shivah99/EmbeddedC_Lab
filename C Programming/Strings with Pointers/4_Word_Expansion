#include <stdio.h>
#include <math.h>
#include<string.h>
int main()
{
    int num, tmp = 0, digits;
    char str[10];
    printf("Enter a String of Numbers: ");
    scanf("%s", str);
    num=atoi(str);  
    printf("Word Expansion of Numbers is:");

    while(num != 0)
    {
        tmp = (tmp * 10) + (num % 10);
        num /= 10;
    }
    while(tmp != 0)
    {
        switch(tmp % 10)
        {
        case 0:
            printf("Zero ");
            break;
        case 1:
            printf("One ");
            break;
        case 2:
            printf("Two ");
            break;
        case 3:
            printf("Three ");
            break;
        case 4:
            printf("Four ");
            break;
        case 5:
            printf("Five ");
            break;
        case 6:
            printf("Six ");
            break;
        case 7:
            printf("Seven ");
            break;
        case 8:
            printf("Eight ");
            break;
        case 9:
            printf("Nine ");
            break;
        }
        tmp /= 10;
    }

    return 0;
}