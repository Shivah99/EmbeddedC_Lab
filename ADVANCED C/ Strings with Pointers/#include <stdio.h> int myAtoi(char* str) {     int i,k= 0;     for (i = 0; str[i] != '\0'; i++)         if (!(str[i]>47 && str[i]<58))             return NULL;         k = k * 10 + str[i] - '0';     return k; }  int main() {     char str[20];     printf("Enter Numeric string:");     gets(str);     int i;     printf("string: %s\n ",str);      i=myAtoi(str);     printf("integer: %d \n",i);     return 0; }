#include <stdio.h>
int myAtoi(char* str)
{
    int i,k= 0;
    for (i = 0; str[i] != '\0'; i++)
        if (!(str[i]>47 && str[i]<58))
            return NULL;
        k = k * 10 + str[i] - '0';
    return k;
}

int main()
{
    char str[20];
    printf("Enter Numeric string:");
    gets(str);
    int i;
    printf("string: %s\n ",str);
     i=myAtoi(str);
    printf("integer: %d \n",i);
    return 0;
}
